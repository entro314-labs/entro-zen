{
  "name": "@entro314labs/entro-zen",
  "version": "1.1.1",
  "description": "Modern, minimalist React component library based on react-zen from umami project",
  "keywords": [
    "entrolytics",
    "analytics",
    "monitoring"
  ],
  "author": "Entrolytics <hey@entrolytics.click>",
  "license": "MIT",
  "homepage": "https://entrolytics.click",
  "repository": {
    "type": "git",
    "url": "https://github.com/entro314-labs/entro-zen.git",
    "directory": "entro-zen"
  },
  "bugs": {
    "url": "https://github.com/entro314labs/entro-zen/issues"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "styles.css",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": [
    "**/*.css",
    "**/*.scss"
  ],
  "engines": {
    "node": ">=22.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prepublishOnly": "rm -rf dist && pnpm bundle",
    "dev": "next dev -p 9001 --turbo",
    "build": "next build",
    "start": "next start",
    "lint": "biome check --write .",
    "lint:check": "biome check .",
    "format": "biome format --write .",
    "icons": "svgr ./src/assets --out-dir src/components/svg --typescript",
    "esbuild": "node esbuild.mjs",
    "rollup": "rollup -c",
    "css": "node css.mjs",
    "types": "tsup",
    "bundle": "npm-run-all types rollup css"
  },
  "dependencies": {
    "@fontsource/jetbrains-mono": "^5.2.8",
    "@internationalized/date": "^3.10.0",
    "@react-aria/focus": "^3.21.2",
    "@react-spring/web": "^10.0.3",
    "@svgr/rollup": "^8.1.0",
    "classnames": "^2.5.1",
    "glob": "^13.0.0",
    "highlight.js": "^11.11.1",
    "lucide-react": "^0.555.0",
    "next": "^16.0.6",
    "react-aria-components": "^1.13.0",
    "react-hook-form": "^7.67.0",
    "react-icons": "^5.5.0",
    "thenby": "^1.3.4",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.8",
    "@hyrious/esbuild-plugin-commonjs": "^0.2.6",
    "@rollup/plugin-alias": "^6.0.0",
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@svgr/cli": "^8.1.0",
    "@types/mdx": "^2.0.13",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@umami/esbuild-plugin-css-modules": "0.4.0",
    "babel-plugin-inline-react-svg": "^2.0.2",
    "dts-bundle": "^0.7.3",
    "esbuild": "^0.27.0",
    "esbuild-css-modules-plugin": "^3.1.5",
    "esbuild-plugin-svgr": "^3.1.1",
    "esbuild-postcss": "^0.0.4",
    "lint-staged": "^16.2.7",
    "npm-run-all": "^4.1.5",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "rollup": "^4.53.3",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-delete": "^3.0.1",
    "rollup-plugin-dts": "^6.3.0",
    "rollup-plugin-esbuild": "^6.2.1",
    "rollup-plugin-postcss": "^4.0.2",
    "stylelint": "^16.26.1",
    "stylelint-config-recommended": "^17.0.0",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,json}": [
      "biome check --write --no-errors-on-unmatched"
    ],
    "**/*.css": [
      "stylelint --fix"
    ]
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "rollup",
      "sharp",
      "unrs-resolver"
    ]
  }
}
