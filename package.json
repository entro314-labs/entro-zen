    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@hyrious/esbuild-plugin-commonjs": "^0.2.6",
    "@rollup/plugin-alias": "^6.0.0",
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@svgr/cli": "^8.1.0",
    "@types/mdx": "^2.0.13",
